@article{ WOS:001146326300004,
Author = {Geelen, H. and Vanryckeghem, V. and Leirs, G.},
Title = {Evaluation of the native femoral neck and stem version reproducibility
   using robotic-arm assisted direct-anterior total hip arthroplasty},
Journal = {ACTA ORTHOPAEDICA BELGICA},
