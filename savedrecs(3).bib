@article{ WOS:001491393700002,
Author = {Rodrigues, Luciano Palmeiro and Candotti, Claudia Tarrago and Goldim,
   Jose Roberto},
Title = {Cross-cultural adaptation, content validity, test-retest reproducibility
   and reliability of the reintegration to normal living index: Brazilian
   Portuguese version},
Journal = {MULTIPLE SCLEROSIS AND RELATED DISORDERS},
Year = {2025},
Volume = {99},
Month = {JUL},
Abstract = {Objective: To translate and cross-culturally adapt the Reintegration to
   Normal Living Index into brazilian portuguese, assess the content
   validity, test-retest reproducibility, and reliability of the instrument
   in individuals with multiple sclerosis. Method: The process of
   translation and crosscultural adaptation of the Reintegration to Normal
   Living Index into brazilian portuguese followed standard guidelines. The
   study was conducted in three stages: a) cross-cultural adaptation; b)
   content validity using the experience of individuals with multiple
   sclerosis; c) reproducibility test-retest (7-day interval) and
   reliability by assessing internal consistency. The Content Validity
   Index (CVI), Intraclass Correlation Coefficient (ICC) and Cronbach's
   alpha were calculated. Results: Fifty patients with an average diagnosis
   time of 10.4 (+8.4) years were evaluated. The I-CVI for all statements
   in the questionnaire was above 0.90. In the analysis of test-retest
   reproducibility and reliability of the RNLI-BP, excellent ICC (0.911)
   and Cronbach's Alpha (0.953) were found. The mean score of the
   Reintegration to Normal Living Index was 76.0 (+23.5) in the first
   assessment and 76.3 (+25.3) in the second assessment. Conclusion: The
   Brazilian Portuguese version of the Reintegration to Normal Living Index
   (RNLI-BP) has content validity, reproducibility and reliability in
   measuring participation in individuals with multiple sclerosis.},
