@article{ WOS:001393365800001,
Author = {Reyes, Sergio E. and Gonzalez-Hernandez, Martin and Cortes, Maria T. and
   Ortiz, Pablo},
Title = {Effect of NH4F surface treatment of electrodeposited NiOx on the
   properties and reproducibility of inverted perovskite solar cells},
Journal = {SURFACES AND INTERFACES},
Year = {2025},
Volume = {56},
Month = {JAN 1},
Abstract = {Inverted perovskite solar cells (IPSC) represent a promising alternative
   for photovoltaic energy generation due to their high power conversion
   efficiency (PCE). Their fabrication makes these devices a cost-effective
   option with significant potential for solar energy generation. Nickel
   oxide (NiOx) has been investigated as a hole-transport layer material in
   these IPSCs due to its high electrical conductivity and its influence on
   PCE stability. Despite previous reports, there are opportunities for
   improvement in the NiOx/Perovskite interface, which are key aspects for
   device performance. This study evaluated the effect of fluoride surface
   treatment on electrochemically deposited (ECD) NiOx from two
   concentrations of an aqueous solution of NH4F, performed via
   spin-coating, on the properties and reproducibility of IPSCs. The
   highest efficiency was achieved under the NiOx-F 1 mg mL-1 condition,
   reaching a PCE of 19.12 \% compared to the reference NiOx which showed a
   PCE of 17.32 \%. This research offers new insights and delves into the
   reasons behind these improvements, showing through electrochemical
   analysis and series resistance studies that the probable causes include
   better charge conduction and high recombination resistance at the
   interface. Finally, the perovskite grain sizes on NiOx-ECD were larger
   than those reported in the literature with colloidal NiOx, contributing
   to greater device stability.},
