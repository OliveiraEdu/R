@article{ WOS:001224820800001,
Author = {Kim, Hyunkyoung and Kong, Ye-Jin and Kim, Won-Suk and Shafian, Shafidah
   and Kim, Kyungkon},
Title = {Enhancing Reproducibility in Organic Solar Cell Fabrication via Static
   Sequential Deposition with Cross-Linked Polymer Donor and Nonfullerene
   Acceptor},
Journal = {ACS APPLIED POLYMER MATERIALS},
Year = {2024},
