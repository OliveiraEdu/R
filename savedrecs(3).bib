@article{ WOS:001444830700001,
Author = {Muta, Ryota and Ishizuka, Chika and Li, Cong and Yoo, Sung-Jun and Ito,
   Kazuhide},
Title = {A numerical simulation-based reproducibility investigation and
   visualization of NSF/ANSI 49 safety performance testing of biological
   safety cabinets},
Journal = {BUILDING AND ENVIRONMENT},
Year = {2025},
Volume = {270},
Month = {FEB 15},
Abstract = {The safety performance of biological safety cabinets (BSC) used in
   laboratory experiments with various microorganisms and chemical
   pollutants shall be regularly examined according to a prescribed test
   method (EN 12,649, NSF/ANSI 49). Performance test methods and procedures
   in the pursuit of safety performance are described in detail.
   Standardized performance test methods evaluate the probability of
   leakage of bioaerosol particles containing Bacillus subtilis. Since the
   main focus of this performance test is to check for leakage outside the
   cabinet, the formation mechanism of bioaerosol concentration
   distributions and capture efficiencies have been not well investigated
   with detailed visualization results. In addition, there is room for
   improvement of energy consumption because the ventilation design of BSC
   considers worst-case scenario, and excessive exhaust airflow is used to
   be on the safe side. In this study, we report the results of a series of
   numerical simulations of contamination control performance that
   accurately reproduce the prescribed safety performance test method for
   BSC using computational fluid dynamics (CFD) and three-dimensional laser
   measurement techniques. The simulation results could visualize the
   non-uniform flow and bioaerosol distribution fields formed inside the
   cabinet under numerical or boundary conditions that met safety standards
   in NSF/ANSI49. The numerical analysis also showed that the performance
   test conditions were not always on the safe side of the evaluation; in
   particular, the risk of leakage became evident when reproducing the
   human body in front of the BSC.},
