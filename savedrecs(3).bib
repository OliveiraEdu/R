@article{ WOS:000650453900007,
Author = {Masiak, Marek and Kotlarska, Iwona and Kondraciuk, Ukasz and Szpindler,
   Maciej},
Title = {Critique of ``Planetary Normal Mode Computation: Parallel Algorithms,
   Performance, and Reproducibility{''} by SCC Team From University of
   Warsaw},
