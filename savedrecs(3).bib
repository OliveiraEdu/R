@article{ WOS:001507489100001,
Author = {Kisi, Sadime Basak and Filskov Petersen, Caroline and Sand Andersen,
   Rikke and Ingemann Rasmussen, Sidse Ida and Parlesak, Alexandr and
   Hojlund Christensen, Sine and Lysdal Petersen, Hanne and Geiker, Nina
   Rica Wium and Friberg Hitz, Mette and Tetens, Inge},
Title = {Validity and reproducibility of a web-based dietary assessment tool: a
   cross-sectional study in an adult Danish population},
Journal = {JOURNAL OF NUTRITIONAL SCIENCE},
Year = {2025},
Volume = {14},
Month = {JUN 4},
Abstract = {This repeated cross-sectional study assessed the validity and
   reproducibility of the myfood24 (R) dietary assessment tool against
   dietary intake biomarkers in healthy Danish adults. The study included
   71 healthy adults (14/57 m/f), aged 53.2 +/- 9.1 years with an average
   BMI of 26.1 +/- 0.3 kg/m(2). Participants were instructed to complete
   seven-day weighed food records using myfood24 (R) at baseline and 4 +/-
   1 weeks thereafter. Estimated mean dietary intake was compared with
   objective measures of energy metabolism and selected dietary intake
   biomarkers in fasting blood (folate) and in 24-hour urine (urea,
   potassium). Resting energy expenditure was measured by indirect
   calorimetry. Application of the Goldberg cut-off classified 87\% (n =
   62) of participants as acceptable reporters. A strong Spearman's rank
   correlation was observed between total folate intake and serum folate
   (rho = 0.62). Acceptable correlations were noted for serum folate (rho =
   0.49) and urinary potassium excretion (rho = 0.44) with estimated and
   measured protein intake (rho = 0.45); energy intake and total energy
   expenditure (rho = 0.38); potassium intake and potassium excretion (rho
   = 0.42); and estimated fruit and vegetable intake. Reproducibility
   analysis revealed strong correlations (rho >= 0.50) across most
   nutrients and food groups, except for fish and vitamin D (rho = 0.30 and
   rho = 0.26, respectively). Notably, reproducibility for folate and total
   vegetable intake exhibited the highest correlations (rho = 0.84 and rho
   = 0.78, respectively). In conclusion, while some limitations exist,
   myfood24 (R) remains a useful tool for ranking individuals by intake,
   particularly in studies focusing on relative comparisons.},
