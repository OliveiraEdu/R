@article{ WOS:001366130300001,
Author = {Panahi, Mehdi and Habibi, Maliheh and Hosseini, Mahboube Sadat},
Title = {Enhancing MRI radiomics feature reproducibility and classification
   performance in Parkinson's disease: a harmonization approach to
   gray-level discretization variability},
Journal = {MAGNETIC RESONANCE MATERIALS IN PHYSICS BIOLOGY AND MEDICINE},
Year = {2025},
Volume = {38},
