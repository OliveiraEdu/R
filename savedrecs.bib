@article{ WOS:000753395300006,
Author = {Etienne, Olivier and Nguyen, Thomas},
Title = {Esthetic mock-ups: a clinical study of reproducibility},
Journal = {INTERNATIONAL JOURNAL OF ESTHETIC DENTISTRY},
