@article{ WOS:001224900100001,
Author = {Richetelli, Nicole and Lemay, Jan and Dunagan, Kensley M. and Parks,
   Connie L. and Hicklin, R. Austin and Chapman, William J.},
Title = {Accuracy and reproducibility of forensic tire examination decisions},
