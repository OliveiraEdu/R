@article{ WOS:001129158200001,
Author = {Zoeger, Manfred and Nimmerichter, Alfred and Baca, Arnold and Wirth,
   Klaus},
Title = {Reproducibility of peak moment for isometric and isokinetic knee
   extension exercise},
