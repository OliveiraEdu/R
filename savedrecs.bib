@article{ WOS:001127330100001,
Author = {Kunpatee, Kanjana and Panphloi, Muthita and Charoenkitamorn, Kanokwan
   and Pimpitak, Umaporna and Puthong, Songchan and Buakeaw, Anumart and
   Komolpis, Kittinan and Sain, Mohini M. and Yakoh, Abdulhadee and Chaiyo,
