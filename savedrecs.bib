@article{ WOS:000612871500002,
Author = {Fuscone, Simone and Favre, Benoit and Prevot, Laurent},
Title = {Reproducibility in speech rate convergence experiments},
Journal = {LANGUAGE RESOURCES AND EVALUATION},
