@article{ WOS:001116676600054,
Author = {Fricano, Fiammetta and Boiron, Hugo and Morana, Adriana and Melin,
   Gilles and Weninger, Luca and Campanella, Cosimo and Boukenter, Aziz and
   Marin, Emmanuel and Ouerdane, Youcef and Robin, Thierry and Girard,
   Sylvain},
Title = {Reproducibility of Dose Rate Measurements With Radioluminescent
   Nitrogen-Doped Optical Fibers},
Journal = {IEEE TRANSACTIONS ON NUCLEAR SCIENCE},
Year = {2023},
Volume = {70},
Number = {8},
Pages = {1925-1932},
Month = {AUG},
Abstract = {We studied the reproducibility of X-ray dose rate measurements using
   radioluminescent (RL) single-mode (SM) and multi-mode (MM) N-doped
   silica-based optical fibers (OFs) in the {[}22 mGy(SiO2)/s-6.6
   Gy(SiO2)/s] range. The influence of the probe manufacturing parameters
   on the dispersion of the RL sensitivity coefficient (chi in counts .
   mGy(-1).s) is discussed for 20 manufactured probes. In particular, we
   demonstrated how the quality control of the splice between the N-doped
   sample and its transport fiber can efficiently reduce the dispersion of
   those coefficients. In addition, also the impact of measuring the RL
   fiber length was studied. Thanks to this study, we could estimate the
   achievable performances of this potential fiber-based dosimeter,
   avoiding the individual calibration procedure of each probe. The good
   performances in terms of chi dispersion (17\% for the MM optical and
   3.8\% for the SM OFs) demonstrate the high potential of this technology
   for implementation in harsh environments.},
