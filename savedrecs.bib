@article{ WOS:000857885900001,
Author = {Rethlefsen, Melissa L. and Norton, Hannah F. and Meyer, Sarah L. and
   MacWilkinson, Katherine A. and Smith, Plato L. and Ye, Hao},
Title = {Interdisciplinary Approaches and Strategies from Research
   Reproducibility 2020: Educating for Reproducibility},
