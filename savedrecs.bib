@article{ WOS:000857241300001,
Author = {Anderson, Lysanna and Presnetsova, Liubov and Wahl, David B. and Phelps,
   Geoffrey and Gous, Alan},
Title = {Assessing reproducibility in sedimentary macroscopic charcoal count data},
