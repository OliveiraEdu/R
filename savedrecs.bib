@article{ WOS:000713163300002,
Author = {Goebel, Rolf J.},
Title = {The Musical Ineffable Revisited: Hermeneutics, Psychoanalysis, and
   Media-Technological Reproducibility},
Journal = {JOURNAL OF MUSICOLOGY},
Year = {2021},
Volume = {38},
Number = {4},
Pages = {419-435},
Month = {FAL},
Abstract = {Inherited from Romantic metaphysics, the musical ineffable is a
   contested category; denoting something in musical experience that
   largely escapes verbal articulation, it also leads to diverse
   philosophical assessments of its validity as an analytic category. Its
   contested status, however, can be given clearer, historically embedded
   contours if it is related to Jacques Lacan's psychoanalytic register of
   the Real across historically changing modes of media-technological
   reproducibility. This connection gives us new insights into the
   interface between sensuous immediacy, verbal articulation, and sonic
   media characteristic of the auditory imagination.},
