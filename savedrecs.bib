@article{ WOS:001193751700003,
Author = {Moassefi, Mana and Singh, Yashbir and Conte, Gian Marco and Khosravi,
   Bardia and Rouzrokh, Pouria and Vahdati, Sanaz and Safdar, Nabile and
   Moy, Linda and Kitamura, Felipe and Gentili, Amilcare and Lakhani, Paras
   and Kottler, Nina and Halabi, Safwan S. and Yacoub, Joseph H. and Hou,
   Yuankai and Younis, Khaled and Erickson, Bradley J. and Krupinski,
