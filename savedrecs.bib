@article{ WOS:001335993800001,
Author = {Matsumoto, Satoshi and Inubushi, Masanobu and Goto, Susumu},
Title = {Stable reproducibility of turbulence dynamics by machine learning},
Journal = {PHYSICAL REVIEW FLUIDS},
Year = {2024},
Volume = {9},
Number = {10},
Month = {OCT 11},
Abstract = {We investigate the stability and accuracy of a machine-learning-based
   turbulence closure model. To this end, we construct a turbulence closure
   model for a shell model, which is a toy model of turbulence, based on
   the inference of sub-grid scale (SGS) variables using a recurrent neural
   network, and conduct an extensive parameter survey of the constructed
   model. The model stably and accurately reproduces the statistics of
   grid-scale variables when the cutoff wave number kappa c is higher than
   0.2ri-1, . 2 ri - 1 , where ri denotes the Kolmogorov length. This is
   because in this case, SGS variables are subordinate to grid-scale ones.
   On the other hand, when kappa c is lower than 0.2ri-1, . 2 ri - 1 , the
   model becomes stochastically unstable. However, an appropriate
   regularization in the inference step of SGS variables realizes a
   sufficiently long lifetime of the model.},
