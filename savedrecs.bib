@article{ WOS:001150363600001,
Author = {LaPolla, Fred Willie Zametkin and Bakker, Caitlin J. and Exner, Nina and
   Montnech, Tisha and Surkis, Alisa and Ye, Hao},
Title = {Rigor and reproducibility instruction in academic medical libraries},
