@article{ WOS:000865949300029,
Author = {Helliwell, John R. and Massera, Chiara},
Title = {The four Rs and crystal structure analysis: reliability,
   reproducibility, replicability and reusability},
Journal = {JOURNAL OF APPLIED CRYSTALLOGRAPHY},
