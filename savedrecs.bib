@article{ WOS:001098051400001,
Author = {Lima, Killian and Castro, Ana C. Meira and Baptista, Joao},
Title = {MIAR<SUP>forest</SUP> Reproducibility and Reliability for Assessing
   Occupational Risks in the Rainforest},
