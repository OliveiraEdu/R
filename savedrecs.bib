@article{ WOS:001132639700001,
Author = {Fisar, Milos and Greiner, Ben and Huber, Christoph and Katok, Elena and
   Ozkes, Ali I. and Management Science Reproducibility},
Title = {Reproducibility in Management Science},
Journal = {MANAGEMENT SCIENCE},
Year = {2024},
Volume = {70},
