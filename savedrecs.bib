@article{ WOS:000760976300020,
Author = {Von Petersdorff-Campen, Kai and Fischer, Peter and Bogdanova, Anna and
   Daners, Marianne Schmid},
Title = {Potential Factors for Poor Reproducibility of In Vitro Hemolysis
   Testing},
Journal = {ASAIO JOURNAL},
