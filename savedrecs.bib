@article{ WOS:000783670200003,
Author = {Zhao, Wenhao and Li, Qiuli and Liu, Yu and Tang, Guoqiang and Ling,
   Xiaoxiao and Li, Jiao and Li, Xianhua},
Title = {Long-Term Reproducibility of SIMS Zircon U-Pb Geochronology},
Journal = {JOURNAL OF EARTH SCIENCE},
Year = {2022},
Volume = {33},
Number = {1, SI},
Pages = {17-24},
Month = {FEB},
Abstract = {Secondary ion mass spectrometry (SIMS) zircon U-Pb dating has been
   widely used to confine the absolute ages of the magmatic or metamorphic
   events and to distinguish multiple events. Here, a data set consisting
