@article{ WOS:001102763800030,
Author = {Jacquerie, P. and Andre, B. and De Seny, D. and Henket, M. and Giltay,
   L. and Ernst, M. and Louis, R. and Malaise, M. and Ribbens, C. and
   Guiot, J.},
Title = {Reproducibility of pulmonary function tests in patients with systemic
   sclerosis},
