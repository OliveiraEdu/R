@article{ WOS:001450687600001,
Author = {Thiren, Philipp and Mehr, Mariam and Eger, Thomas and Deschner, James
   and Geyer, Andreas Magnus},
Title = {Reproducibility of Digital Measurements on Soft and Hard Tissue},
