@article{ WOS:001087960100001,
Author = {Soliman, Nadia and Denk, Franziska},
Title = {Practical approaches to improving translatability and reproducibility in
   preclinical pain research},
Journal = {BRAIN BEHAVIOR AND IMMUNITY},
Year = {2024},
Volume = {115},
Pages = {38-42},
Month = {JAN},
Abstract = {Pain research continues to face the challenge of poor translatability of
   pre-clinical studies. In this short primer, we are summarizing the
   possible causes, with an emphasis on practical and constructive
   solutions. In particular, we stress the importance of increased
   heterogeneity in animal studies; formal or informal pre-registration to
   combat publication bias; and increased statistical training in order to
   help pre-clinical scientists appreciate the usefulness of available
   experi-mental design and reporting guidelines.},
