@article{ WOS:000860573000004,
Author = {Maloy, Abigail and Alexander, Shaun and Andreas, Alexis and Nyunoya,
   Toru and Chandra, Divay},
Title = {Stain-Free total-protein normalization enhances the reproducibility of
   Western blot data},
Journal = {ANALYTICAL BIOCHEMISTRY},
Year = {2022},
Volume = {654},
Month = {OCT 1},
Abstract = {We compared the accuracy of three common methods of total protein
   normalization. The Stain-Free method was accurate across different
   types/brands of western blotting membrane and for various protein loads,
   unlike Ponceau S and Amido Black. Normalizing to the housekeeping
   proteins Actin and 13-Tubulin could match the accuracy of the Stain-Free
   method. However, compared to Actin or 13-Tubulin, normalizing to the
   Stain-Free signal reduced variability that led to enhanced
   reproducibility and a reduction in the number of samples needed to
   obtain statistically significant results by >50\%. Stain-Free
   normalization can enhance the reproduc-ibility and hence the confidence
   in Western Blot data.},
