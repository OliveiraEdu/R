@article{ WOS:000743438600001,
Author = {Simileysky, Alexander and Hull, Maury L.},
Title = {Repeatability, reproducibility, and agreement of three methods for
   finding the mechanical axis of the human tibia},
Journal = {COMPUTER METHODS IN BIOMECHANICS AND BIOMEDICAL ENGINEERING},
