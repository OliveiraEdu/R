@article{ WOS:001273934200011,
Author = {Zhang, Shujiao and Zhang, Zunyue and Guo, Rongxiang and Xu, Yingqi and
   Chen, Si and Wang, Jiaqi and Liu, Tiegen and Cheng, Zhenzhou},
Title = {Silicon Subwavelength Grating Coupler With Ultra-High Reproducibility,
   Ultra-Wide Bandwidth, and Ultra-Low Back Reflection},
Journal = {JOURNAL OF LIGHTWAVE TECHNOLOGY},
Year = {2024},
