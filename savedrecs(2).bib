Address = {1155 16TH ST, NW, WASHINGTON, DC 20036 USA},
Type = {Article},
Language = {English},
Affiliation = {Landfester, K (Corresponding Author), Max Planck Inst Polymer Res, D-55128 Mainz, Germany.
