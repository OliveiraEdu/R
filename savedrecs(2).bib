@article{ WOS:001158776900001,
Author = {Varol, Umut and Sanchez-Jimenez, Elena and Valera-Calero, Juan Antonio
   and Plaza-Manzano, Gustavo and Fernandez-de-las-Penas, Cesar and
   Navarro-Santana, Marcos Jose and Sanchez-Jorge, Sandra and
   Ortega-Santiago, Ricardo},
Title = {Reproducibility of Anterior Scalene Stiffness Measurement with Shear
   Wave Elastography: An Inter-Examiner Reliability Study},
Journal = {ULTRASCHALL IN DER MEDIZIN},
Year = {2024},
Volume = {45},
