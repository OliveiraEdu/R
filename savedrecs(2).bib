@article{ WOS:001149836900012,
Author = {Fedorov, Andrey and Longabaugh, William J. R. and Pot, David and Clunie,
   David A. and Pieper, Steven D. and Gibbs, David L. and Bridge,
   Christopher and Herrmann, Markus D. and Homeyer, Andre and Lewis, Rob
