@article{ WOS:001467993400001,
Author = {Liu, Ruo-Tao and Tang, Qian and Zhang, Jin-Shan and Hu, Zhi-Chao and
   Lin, Zi-Jun and Ye, Zhen and Zhou, Chao-Zheng and Zheng, Yong-Qiang and
   Zhang, Chang-Qing and Zheng, Guo-Yan and Zhu, Zhen-Zhong},
Title = {Robot-Based Minimally Invasive Spherical Osteotomy System Improves the
   Accuracy and Reproducibility of the Resulting Surface},
Journal = {INTERNATIONAL JOURNAL OF MEDICAL ROBOTICS AND COMPUTER ASSISTED SURGERY},
Year = {2025},
