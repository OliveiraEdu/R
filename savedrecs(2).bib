@article{ WOS:000712246800001,
Author = {Thompson, Michael and Sykes, Mark and Mathieson, Kenneth and Wood, Roger},
Title = {Comparison of reproducibility precision on mass fraction in some
   interlaboratory studies of methods of food analysis},
