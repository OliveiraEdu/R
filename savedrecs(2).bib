@article{ WOS:000642365800001,
Author = {Hudson, Robert},
Title = {Should We Strive to Make Science Bias-Free? A Philosophical Assessment
   of the Reproducibility Crisis},
Journal = {JOURNAL FOR GENERAL PHILOSOPHY OF SCIENCE},
