@article{ WOS:001433754000001,
Author = {Yuan, Yu and Wan, Biao and Shen, Weixia and Zhang, Zhuangfei and Fang,
   Chao and Wang, Qianqian and Chen, Liangchao and Zhang, Yuewen},
Title = {Sulfur-alloying ensures the reproducibility of p-type cubic AgSnBiSe3
   thermoelectric material with low lattice thermal conductivity},
Journal = {JOURNAL OF ALLOYS AND COMPOUNDS},
Year = {2025},
Volume = {1020},
Month = {MAR 15},
Abstract = {Configurational entropy engineering is generally performed to achieve
   cubic phase and high thermoelectric performance. In this work, cubic
   AgSnBiSe3 is obtained by alloying AgBiSe2 and SnSe with molar ratio of
   1:1. However, AgSnBiSe3 samples exhibit distinct property
   irreproducibility due to the uncontrollable lattice defects. Thus,
   sulfur alloying on Se sites is proposed to achieve repeatable synthesis
   and to stabilize the thermoelectric performance. The p-type
   AgSnBiSe2.5S0.5 sample exhibits a low lattice thermal conductivity of
   0.40-0.55 Wm- 1K- 1 in temperature range of 300-673 K due to the
   cation/anion disorder and local structural distortion. Cu substitution
   at Bi site is further performed to optimize the carrier concentration
   and effective mass. The peak values of Seebeck coefficient and power
   factor shift from room temperature to higher temperature, which improves
   the average zT value from 0.056 to 0.147 after Cu doping. A maximum zT
   value of 0.19 at 473 K is obtained for AgSnBi0.95Cu0.05Se2.5S0.5 sample,
   indicating the thermoelectric potential of cubic compounds in AgBiSe2-
   SnSe system.},
