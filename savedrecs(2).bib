@article{ WOS:000829786800007,
Author = {Xu, S. and Li, Z. and Hu, J. and Zhang, J.},
Title = {Statistical process control tools for setup reproducibility in quality
   control practices during nasopharyngeal carcinoma radiotherapy},
Journal = {INTERNATIONAL JOURNAL OF RADIATION RESEARCH},
Year = {2022},
