@article{ WOS:000741400400001,
Author = {Pourmotabbed, Haatef and Curry, Amy L. de Jongh and Clarke, Dave F. and
   Tyler-Kabara, Elizabeth C. and Babajani-Feremi, Abbas},
Title = {Reproducibility of graph measures derived from resting-state MEG
   functional connectivity metrics in sensor and source spaces},
Journal = {HUMAN BRAIN MAPPING},
Year = {2022},
