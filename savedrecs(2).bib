@article{ WOS:000833254400001,
Author = {Haange, Sven-Bastiaan and Till, Andreas and Bergh, Per-Olof and Fauler,
   Guenter and Gigl, Michael and Loefgren-Sandblom, Anita and Schaap, Frank
   G. and Clavel, Thomas and Trautwein, Christian and Fenske, Wiebke and
   Kleigrewe, Karin and Marschall, Hanns-Ulrich and Damink, Steven W. M.
   Olde and Moustafa, Tarek and von Bergen, Martin and Rolle-Kampczyk,
   Ulrike},
Title = {Ring Trial on Quantitative Assessment of Bile Acids Reveals a Method-
   and Analyte-Specific Accuracy and Reproducibility},
Journal = {METABOLITES},
Year = {2022},
Volume = {12},
Number = {7},
Month = {JUL},
Abstract = {Bile acids are a key mediator of the molecular microbiome-host
   interaction, and various mass spectrometry-based assays have been
   developed in the recent decade to quantify a wide range of bile acids.
   We compare existing methodologies to harmonize them. Methodology for
   absolute quantification of bile acids from six laboratories in Europe
   were compared for the quantification of the primary bile acids cholic
   acid (CA) and chenodeoxycholic acid (CDCA) and conjugated products
   glycocholic acid (GCA) and taurocholic acid (TCA). For the bacterially
   modified secondary bile acids, the quantification of deoxycholic acid
   (DCA) and lithocholic acid (LCA) was compared. For the murine bile
   acids, we used the primary muricholic acids (alpha-MCA and, beta-MCA)
   and the intestinally produced secondary bile acid muricholic
   (omega-MCA). The standards were spiked into methanol:water (1:1) mix as
   well as in human and murine serum at either low concentration range
   (150-3000 nM) or high concentration range (1500-40,000 nM). The
   precision was better for higher concentrations. Measurements for the
   hydrophobic unconjugated bile acids LCA and omega-MCA were the most
   challenging. The quality assessments were generally very similar, and
   the comprehensive analyses demonstrated that data from chosen locations
   can be used for comparisons between studies.},
Publisher = {MDPI},
Address = {ST ALBAN-ANLAGE 66, CH-4052 BASEL, SWITZERLAND},
