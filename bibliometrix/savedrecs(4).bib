@article{ WOS:001177962000001,
Author = {Sadeghi, Seyed Amirhossein and Chen, Wenrong and Wang, Qianyi and Wang,
   Qianjie and Fang, Fei and Liu, Xiaowen and Sun, Liangliang},
Title = {Pilot Evaluation of the Long-Term Reproducibility of Capillary Zone
   Electrophoresis-Tandem Mass Spectrometry for Top-Down Proteomics of a
   Complex Proteome Sample},
