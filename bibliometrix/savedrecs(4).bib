@article{ WOS:001367895800001,
Author = {Erivan, Roger and Urbain, Antoine and Santorum, Tony and Giordano,
   Gerard and Reina, Nicolas and Bonnomet, Francois and Jenny, Jean Yves
   and Peuchot, Henri and Bonin, Nicolas and Hormi-Menard, Mehdi and
