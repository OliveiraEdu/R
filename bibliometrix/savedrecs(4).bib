@article{ WOS:000880189100025,
Author = {Rohrmoser, A. and Kraus, M. and Leicht, M. and Merklein, M.},
Title = {Experimental Evaluation of the Feasibility and Reproducibility of a
   Novel Multi-step Forming Process for Cu-OFE Micro Gear Manufacturing
   Compared to Macro-scale Forming},
