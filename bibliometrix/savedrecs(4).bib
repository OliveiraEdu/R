@article{ WOS:001243766500001,
Author = {Longino, August A. and Martin, Katharine C. and Leyba, Katarina R. and
   Mccormack, Luke and Siegel, Gabriel and Sharma, Vibhu M. and Riscinti,
   Matthew and Lopez, Carolina O. and Douglas, Ivor S. and Gill, Edward A.},
Title = {Reliability and reproducibility of the venous excess ultrasound (VExUS)
   score, a multi-site prospective study: validating a novel ultrasound
   technique for comprehensive assessment of venous congestion},
Journal = {CRITICAL CARE},
Year = {2024},
Volume = {28},
Number = {1},
Month = {JUN 11},
Abstract = {Though the novel venous excess ultrasound (VExUS) score is increasingly
   used as a noninvasive means of venous congestion measurement, the
   inter-rater reliability (IRR), inter-user reproducibility (IUR), and
   utility of concurrent ECG have not been evaluated. We conducted a
   multicenter study of the IRR, IUR, and utility of ECG for VExUS
   interpretation between four attending physicians of diverse specialties,
   reporting the Kappa statistic (KS) and Intraclass Correlation
   Coefficient (ICC) for IRR and IUR for scans with and without ECG.
   Eighty-four paired VExUS exams from 42 patients, 60 of which had a
   concurrent ECG tracing, were interpreted. They showed substantial IRR,
   with a KS of 0.71 and ICC of 0.83 for the overall VExUS grade (p <
   0.001), and IUR, with a KS 0.63 and ICC of 0.8. There was greater
   agreement among images with an ECG tracing. These results suggest that
   ECG-augmented VExUS may be a reliable and reproducible measure
   interpretable by clinicians with diverse backgrounds.},
