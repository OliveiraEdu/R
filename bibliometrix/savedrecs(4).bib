   Arruda-Carvalho, Maithe},
Title = {Promoting and Optimizing the Use of 3D-Printed Objects in Spontaneous
   Recognition Memory Tasks in Rodents: A Method for Improving Rigor and
   Reproducibility},
Journal = {ENEURO},
Year = {2021},
