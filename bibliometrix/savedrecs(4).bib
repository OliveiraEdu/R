@article{ WOS:001508636700005,
Author = {Porquet, Julien},
Title = {How to do things with style: A pragmatic history of style as a site for
   debates about authorship, commodification and technological
   reproducibility},
Journal = {JOURNAL OF ILLUSTRATION},
Year = {2025},
Volume = {12},
