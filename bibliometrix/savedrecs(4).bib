@article{ WOS:001238173200001,
Author = {Sakoda, Hideyuki and Tamazawa, Kengo and Shoyama, Yuichi and Osaka, Yuta
   and Uetsuki, Keita and Okamoto, Yoshihiro and Yamamoto, Eiichi},
Title = {Sensitivity, robustness, and reproducibility of U-shaped delamination
   test for evaluation of candidate ultra-high molecular weight
   polyethylene materials for joint replacements},
Journal = {PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART H-JOURNAL OF
   ENGINEERING IN MEDICINE},
