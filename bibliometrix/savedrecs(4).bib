@article{ WOS:001079159400002,
Author = {Lam, Marnix and Garin, Etienne and Palard-Novello, Xavier and Mahvash,
   Armeen and Kappadath, Cheenu and Haste, Paul and Tann, Mark and
   Herrmann, Ken and Barbato, Francesco and Geller, Brian and Schaefer,
   Niklaus and Denys, Alban and Dreher, Matthew and Fowers, Kirk D. and
   Gates, Vanessa and Salem, Riad},
Title = {Direct comparison and reproducibility of two segmentation methods for
   multicompartment dosimetry: round robin study on radioembolization
   treatment planning in hepatocellular carcinoma},
Journal = {EUROPEAN JOURNAL OF NUCLEAR MEDICINE AND MOLECULAR IMAGING},
Year = {2023},
Volume = {51},
Number = {1},
Pages = {245-257},
Month = {DEC},
Abstract = {Purpose: Investigate reproducibility of two segmentation
   methods for multicompartment dosimetry, including normal tissue absorbed
   dose (NTAD) and tumour absorbed dose (TAD), in hepatocellular carcinoma
   patients treated with yttrium-90 (Y-90) glass
   microspheres.Methods: TARGET was a retrospective
   investigation in 209 patients with < 10 tumours per lobe and at least
   one tumour >= 3 cm +/- portal vein thrombosis. Dosimetry was compared
   using two distinct segmentation methods: anatomic (CT/MRI-based) and
   count threshold-based on pre-procedural Tc-99m-MAA SPECT. In a round
   robin substudy in 20 patients with <= 5 unilobar tumours, the
   inter-observer reproducibility of eight reviewers was evaluated by
   computing reproducibility coefficient (RDC) of volume and absorbed dose
   for whole liver, whole liver normal tissue, perfused normal tissue,
   perfused liver, total perfused tumour, and target lesion. Intra-observer
   reproducibility was based on second assessments in 10 patients >= 2
   weeks later.Results: Tc-99m-MAA segmentation calculated
   higher absorbed doses compared to anatomic segmentation (n = 209),
   43.9\% higher for TAD (95\% limits of agreement {[}LoA]: - 49.0\%,
   306.2\%) and 21.3\% for NTAD (95\% LoA: - 67.6\%, 354.0\%). For the
   round robin substudy (n = 20), inter-observer reproducibility was better
   for anatomic (RDC range: 1.17 to 3.53) than Tc-99m-MAA SPECT
   segmentation (1.29 to 7.00) and similar between anatomic imaging
   modalities (CT: 1.09 to 3.56; MRI: 1.24 to 3.50). Inter-observer
   reproducibility was better for larger volumes. Perfused normal tissue
   volume RDC was 1.95 by anatomic and 3.19 by Tc-99m-MAA SPECT, with
   corresponding absorbed dose RDC 1.46 and 1.75. Total perfused tumour
   volume RDC was higher, 2.92 for anatomic and 7.0 by Tc-99m-MAA SPECT
   with corresponding absorbed dose RDC of 1.84 and 2.78. Intra-observer
   variability was lower for perfused NTAD (range: 14.3 to 19.7 Gy) than
   total perfused TAD (range: 42.8 to 121.4 Gy).Conclusion:
   Anatomic segmentation-based dosimetry, versus Tc-99m-MAA
   segmentation, results in lower absorbed doses with superior
   reproducibility. Higher volume compartments, such as normal tissue
   versus tumour, exhibit improved reproducibility.},
