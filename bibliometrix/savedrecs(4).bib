@article{ WOS:000820165600001,
Author = {Liu, Qingshi and Xu, Lanlan and Wang, Ruichen and Gong, Jian and Tang,
   Yawei and Liu, Xiaojuan},
Title = {Kinetic Atomic-Scale Reproducibility of the Oxygen Reduction Reaction
   Process and a Newly Suggested Strong Correlation Descriptor: A Case
   Study of BaCo0.75Fe0.25O3-δ},
