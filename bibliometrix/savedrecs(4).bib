@article{ WOS:001466712200004,
Author = {Ahmadian, Narjes and Jacobs, Sarah M. and Gosselink, Mark and van Der
   Kemp, Wybe J. M. and Hoogduin, Hans and Coppoli, Anastasia and Mason,
   Graeme F. and de Graaf, Robin A. and Norouzizadeh, Helia and Mahon,
   Chantal and van Eijsden, Pieter and Tiessen, Renger and Cerneus, Dirk
   and Miller, Corin O. and De Lepeleire, Inge and Basile, Anthony S. and
   Klomp, Dennis W. J. and Prompers, Jeanine J. and Wiegers, Evita C.},
Title = {Reproducibility of the Determination of <SUP>13</SUP>C-Labeling of
   Glutamate and Glutamine in the Human Brain Using selPOCE-MRS at 7 T Upon
   {[}U-<SUP>13</SUP>C]-Labeled Glucose Infusion},
Journal = {NMR IN BIOMEDICINE},
Year = {2025},
Volume = {38},
Number = {5},
Month = {MAY},
Abstract = {Glutamate (Glu) is the major excitatory neurotransmitter in the central
   nervous system. The measurement of Glu/glutamine (Gln) neurotransmitters
   in the brain provides valuable insights into the dynamic aspects of
   neuroenergetics and neurotransmitter cycles and can be accomplished
   through the detection of C-13-labeling of Glu and Gln during the
   administration of C-13-labeled glucose. Our goal is to evaluate the
   reproducibility of selective proton-observed, carbon-edited (selPOCE)
   MRS at 7 T for the detection of C-13-labeled Glu and Gln in the human
   brain. Data of three healthy participants, who were scanned twice at 7 T
   while undergoing {[}U-C-13]-glucose infusion for 120 min, were used to
   detect C-13-labeled Glu and Gln in the brain, using selPOCE-STEAM-MRS.
   There was a rapid increase of plasma glucose C-13 fractional enrichment
   (FE) during the first 20 min of infusion, followed by a steady state of
   plasma glucose C-13 FE until the end of the {[}U-C-13]-glucose infusion.
   The time courses of C-13-labeling of Glu and Gln were similar for
   test/retest. The test/retest variability was 15.8\% for C-13-Glu and
   33.3\% for C-13-Gln. Knowing the variability of these readings using
   selPOCE-STEAM-MRS can inform the application to future studies on
   disease-specific alterations in Glu/Gln cycling.},
