@article{ WOS:000942278700001,
Author = {Astolfi, Michele and Rispoli, Giorgio and Gherardi, Sandro and Zonta,
   Giulia and Malagu, Cesare},
Title = {Reproducibility and Repeatability Tests on (SnTiNb)O2 Sensors
   in Detecting ppm-Concentrations of CO and Up to 40\% of Humidity: A
   Statistical Approach},
