@article{ WOS:001473590100007,
Author = {Barnes, Heidi and Aygun, Kemal and Hill, Michael J. and Zhang, Zhichao
   and Hu, Kaisheng and Aronsson, Jonatan and Paladhi, Pavel and
   Balachandran, Jayaprakash and Shi, Bobi and Sharma, Rohit and
   Schutt-Aine, Jose E. and Okhmatovski, Vladimir I.},
Title = {Benchmarking and Reproducibility in Computational and Experimental
   Characterization of Electronic Packages for Signal/Power Integrity: Four
   benchmarks serve as standardized cases},
Journal = {IEEE ANTENNAS AND PROPAGATION MAGAZINE},
Year = {2025},
Volume = {67},
