@article{ WOS:001198833600001,
Author = {Liu, Mengqi and Zhao, Shuqiang and Chen, Zhiye},
Title = {Interscanner reproducibility of volumetric quantitative susceptibility
   mapping about cerebral subcortical gray nuclei at different MR vendors
   with the same magnetic strength},
