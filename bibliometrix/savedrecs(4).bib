@article{ WOS:001499724600001,
Author = {Mitchell, Braden L. and Davison, Kade and Parfitt, Gaynor and Eston,
   Roger G.},
Title = {Effect of β-blockade on measures and reproducibility of heart rate,
   oxygen uptake and work rate across repeated bouts of short-duration,
   RPE-regulated exercise},
Journal = {EUROPEAN JOURNAL OF APPLIED PHYSIOLOGY},
Year = {2025},
Month = {2025 JUN 1},
Abstract = {Purpose We examined the effect of beta-blockade on measures and
   reproducibility of heart rate (HR), oxygen uptake (V(center dot)O2) and
   work rate (WR) across repeated bouts of short-duration, RPE-regulated
   exercise. Methods Participants completed an RPE-regulated,
   interval-based exercise session under control and beta-blockade
   conditions with six 3-min bouts alternating between RPE 13 and RPE 15,
   separated by 2-min active recovery periods. Participants adjusted
   treadmill speed/grade to meet the target RPE. Linear mixed effects
   models assessed the effect beta-blockade on exercise responses for each
   RPE, while intraclass correlation coefficients (ICC) and coefficients of
   variation (CV) evaluated reproducibility across bouts. Results
   beta-Blockade significantly reduced HR (- 36.5 beat min(-1), p < 0.001),
   VO2 (- 4.2 mL kg(-1) min(-1), p < 0.001) and work rate (- 0.6 METs, p =
   0.022) during exercise. Differences between conditions remained
   significant for \%HRpeak (p < 0.001) but not \%(VO2peak)-O-center dot or
   \%WRpeak (p > 0.05). Exercise responses were consistently higher at RPE
   15 than RPE 13 (all p < 0.001). A significant interaction showed greater
   HR reduction at RPE 15 (45.5 beat min(-1)) than RPE 13 (40.0 beat
   min(-1)) under beta-blockade (p = 0.041). ICC values indicated good to
   excellent reproducibility across bouts, with no significant difference
   between conditions. Variability across bouts was low (mean CV = 2-8\%)
   and unaffected by beta-blockade. Conclusion Our findings suggest that
   despite significant reductions in absolute responses, beta-blockade does
   not affect relative measures of VO2 or work rate. RPE-regulated exercise
   may facilitate highly reproducible exercise intensities, making it
   particularly valuable for populations where beta-blocker use is
   prevalent.},
