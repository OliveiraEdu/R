@article{ WOS:000604961500011,
Author = {Evans, Neil A. and Konz, Suzanne and Nitz, Arthur and Uhl, Tim L.},
Title = {Reproducibility and discriminant validity of the Posterior Shoulder
   Endurance Test in healthy and painful populations},
