@article{ WOS:001270804800001,
Author = {Wang, Chenyue and He, Bingchen and Hui, Wei and Su, Zhenhuang and Chen,
   Li and Du, Weizhi and Zhang, Liujiang and Zhang, Junhan and Fu, Meirong
   and Wang, Hao and Sun, Bo and Wen, Wen and Wang, Sisheng and Lou, Liyu
   and Hou, Chen and Zheng, Guanhaojie and Song, Lin and Chen, Yonghua and
   Gao, Xingyu},
Title = {Controlling Water for Enhanced Homogeneities in Perovskite Solar Cells
   with Remarkable Reproducibility},
Journal = {ADVANCED FUNCTIONAL MATERIALS},
Year = {2024},
Volume = {34},
Number = {41},
Month = {OCT},
Abstract = {Two-step sequential deposition of perovskite films is proven efficient
   and compatible with large-scale production. Nevertheless, its critically
   regulated environmental humidity often remains a burden, hindering its
   applications in reproducible fabrication of perovskite solar cells
   (PSCs). Herein, by introducing a water ethyl acetate (W-EA) solution
   treatment before annealing, improved perovskite crystallinity and
   homogeneity in FA0.95-xMAxCs0.05PbI3 films without additional
   composition regulation is achieved. In situ Grazing-incidence Wide-angle
   X-ray Scattering experiments highlighted accelerated crystallization
   kinetics in W-EA-treated films, leading to high-quality perovskite
   formation. PSCs based on this method reached an optimal power conversion
   efficiency of 25.01\% and demonstrated enhanced moisture resistance,
   retaining 88\% efficiency after 1000 h in ambient conditions. This W-EA
   treatment simplifies the fabrication process, greatly reducing the need
   for controlled humidity, and offers insights into the role of water in
   perovskite film crystallization, with significant implications to
   produce high-performance PSCs.
   Introducing a water ethyl acetate solution treatment before annealing
   improved perovskite crystallinity and homogeneity in FAMACsPbI films.
   The film improvements are attributed to trace water assisting organic
   salts to permeate homogenously into the PbI2 film. PSCs reached an
   optimal power conversion effecncy of 25.01\% with enhanced moisture
   resistance. This treatment simplifies the fabrication by greatly
   reducing the need for controlled humidity. image},
