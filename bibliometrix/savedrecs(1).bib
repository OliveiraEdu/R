@article{ WOS:001210399500001,
Author = {Farley, Nicholas and Susnjar, Antonia and Chiew, Mark and Emir, Uzay E.},
Title = {Test-Retest Reproducibility of Reduced-Field-of-View Density-Weighted
   CRT MRSI at 3T},
Journal = {TOMOGRAPHY},
