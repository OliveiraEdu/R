@article{ WOS:001050967700001,
Author = {Xu, Zhishuai and Liu, Li and Hao, Zhongqi and Deng, Zhiwei and Lu, Ying
   and Zhao, Ziyi and Li, Jiaming and Shi, Jiulin and He, Xingdao},
Title = {Improvement in detection reproducibility of laser-induced breakdown
   spectroscopy based on plasma acoustic correction},
Journal = {JOURNAL OF ANALYTICAL ATOMIC SPECTROMETRY},
