@article{ WOS:000746568600010,
Author = {de Lima E Silva, Leandro and Neves, Eduardo Borba and Valente dos
   Santos, Luciano Alonso and Da Silva, Jurandir Babtista and De Souza
   Vale, Rodrigo Gomes and Moreira Nunes, Rodolfo de Alkmim},
Title = {THE REPRODUCIBILITY OF THE 1HZ GPS IN THE ACICLIC DISPLACEMENT WITH
   INTERMITTENT INTENSITY},
Journal = {ARCHIVOS DE MEDICINA},
Year = {2021},
