@article{ WOS:000720784600021,
Author = {dos Santos Tenorio, Micaely Cristina and Wanderley, Thiago Marques and
   Macedo, Isadora Albuquerque and Ataide Vanderlei, Alanna Lira and de
   Souza, Bianca Gomes and Ataide-Silva, Thays and Menezes de Oliveira,
