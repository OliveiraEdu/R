@article{ WOS:001217138700001,
Author = {Li, Kangming and Decost, Brian and Choudhary, Kamal and
   Hattrick-Simpers, Jason},
Title = {A reproducibility study of atomistic line graph neural networks for
   materials property prediction},
Journal = {DIGITAL DISCOVERY},
