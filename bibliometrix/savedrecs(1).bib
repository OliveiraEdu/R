@article{ WOS:000822436100046,
Author = {Ferrari, Arianna and Peters, Josh and Anikeeva, Mariia and Pravdivtsev,
   Andrey and Ellermann, Frowin and Them, Kolja and Will, Olga and Peschke,
   Eva and Yoshihara, Hikari and Jansen, Olav and Hoevener, Jan-Bernd},
Title = {Performance and reproducibility of <SUP>13</SUP>C and <SUP>15</SUP>N
   hyperpolarization using a cryogen-free DNP polarizer},
Journal = {SCIENTIFIC REPORTS},
Year = {2022},
Volume = {12},
Number = {1},
Month = {JUL 8},
Abstract = {The setup, operational procedures and performance of a cryogen-free
   device for producing hyperpolarized contrast agents using dissolution
   dynamic nuclear polarization (dDNP) in a preclinical imaging center is
   described. The polarization was optimized using the solid-state,
   DNP-enhanced NMR signal to calibrate the sample position, microwave and
   NMR frequency and power and flip angle. The polarization of a standard
   formulation to yield similar to 4 mL, 60 mM 1-C-13-pyruvic acid in an
   aqueous solution was quantified in five experiments to P(C-13) = (38 +/-
   6) \% (19 +/- 1) s after dissolution. The mono-exponential time constant
   of the build-up of the solid-state polarization was quantified to (1032
   +/- 22) s. We achieved a duty cycle of 1.5 h that includes sample
   loading, monitoring the polarization build-up, dissolution and
   preparation for the next run. After injection of the contrast agent in
   vivo, pyruvate, pyruvate hydrate, lactate, and alanine were observed, by
   measuring metabolite maps. Based on this work sequence, hyperpolarized
   N-15 urea was obtained (P(N-15) = (5.6 +/- 0.8) \% (30 +/- 3) s after
   dissolution).},
