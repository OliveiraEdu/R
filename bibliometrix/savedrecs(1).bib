@article{ WOS:000810774500005,
Author = {Falak, Samreen and Macdonald, Rainer and Busby, Eloise J. and
   O'Sullivan, Denise M. and Milavec, Mojca and Plauth, Annabell and
   Kammel, Martin and Zeichhardt, Heinz and Grunert, Hans-Peter and
   Kummrow, Andreas and Huggett, Jim F.},
Title = {An assessment of the reproducibility of reverse transcription digital
   PCR quantification of HIV-1},
Journal = {METHODS},
Year = {2022},
Volume = {201},
