@article{ WOS:001003513800001,
Author = {Roman-Palacios, Cristian},
Title = {The phruta r package: Increasing access, reproducibility and
   transparency in phylogenetic analyses},
Journal = {METHODS IN ECOLOGY AND EVOLUTION},
