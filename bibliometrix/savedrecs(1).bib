   Liu, Yunxiao and Zhang, Jianming and Yu, Qing and Cheng, Guiqing and
   Wang, Zhongwei},
Title = {Achieving superior pyroresistive reproducibility at HDPE/PVDF composites
   with tailored conductive phase size},
Journal = {POLYMER COMPOSITES},
Year = {2024},
