@article{ WOS:000809480300001,
Author = {Raz, Ofir and Tao, Liming and Biezuner, Tamir and Marx, Tzipy and
   Neumeier, Yaara and Tumanyan, Narek and Shapiro, Ehud},
Title = {Whole-Genome Amplification-Surveying Yield, Reproducibility, and
   Heterozygous Balance, Reported by STR-Targeting MIPs},
Journal = {INTERNATIONAL JOURNAL OF MOLECULAR SCIENCES},
Year = {2022},
Volume = {23},
Number = {11},
Month = {JUN},
Abstract = {Whole-genome amplification is a crucial first step in nearly all
   single-cell genomic analyses, with the following steps focused on its
   products. Bias and variance caused by the whole-genome amplification
   process add numerous challenges to the world of single-cell genomics.
   Short tandem repeats are sensitive genomic markers used widely in
   population genetics, forensics, and retrospective lineage tracing. A
   previous evaluation of common whole-genome amplification targeting
   similar to 1000 non-autosomal short tandem repeat loci is extended here
   to similar to 12,000 loci across the entire genome via duplex molecular
   inversion probes. Other than its improved scale and reduced noise, this
   system detects an abundance of heterogeneous short tandem repeat loci,
   allowing the allelic balance to be reported. We show here that while the
   best overall yield is obtained using RepliG-SC, the maximum uniformity
   between alleles and reproducibility across cells are maximized by
   Amplil, rendering it the best candidate for the comparative heterozygous
   analysis of single-cell genomes.},
