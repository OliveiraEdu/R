@article{ WOS:001292674100001,
Author = {Jogi, Sandeep Panwar and Peng, Qi and Jafari, Ramin and Otazo, Ricardo
   and Wu, Can},
Title = {Novel spin-lock time sampling strategies for improved reproducibility in
   quantitative T1ρ mapping},
Journal = {NMR IN BIOMEDICINE},
Year = {2024},
Volume = {37},
Number = {12},
Month = {DEC},
Abstract = {This study aimed to optimize the sampling of spin-lock times (TSLs) in
   quantitative T1 rho mapping for improved reproducibility. Two new TSL
   sampling schemes were proposed: (i) reproducibility-guided random
   sampling (RRS) and (ii) reproducibility-guided optimal sampling (ROS).
   They were compared to the existing linear sampling (LS) and
   precision-guided sampling (PS) schemes for T1 rho reproducibility
   through numerical simulations, phantom experiments, and volunteer
   studies. Each study evaluated the four sampling schemes with three
   commonly used T1 rho preparations based on composite and balanced
   spin-locking. Additionally, the phantom and volunteer studies
   investigated the impact of B-0 and B-1 field inhomogeneities on T1 rho
   reproducibility, respectively. The reproducibility was assessed using
   the coefficient of variation (CoV) by repeating the T1 rho measurements
   eight times for phantom experiments and five times for volunteer
   studies. Numerical simulations resulted in lower mean CoVs for the
   proposed RRS (1.74\%) and ROS (0.68\%) compared to LS (2.93\%) and PS
   (3.68\%). In the phantom study, the mean CoVs were also lower for RRS
   (2.7\%) and ROS (2.6\%) compared to LS (4.1\%) and PS (3.1\%).
   Furthermore, the mean CoVs of the proposed RRS and ROS were
   statistically lower (P < 0.001) compared to existing LS and PS schemes
   at a B-1 offset of 20\%. In the volunteer study, consistently lower mean
   CoVs were observed in bilateral thigh muscles for RRS (9.3\%) and ROS
   (9.2\%) compared to LS (10.9\%) and PS (10.2\%), and the difference was
   more prominent at B0 offsets higher than 50 Hz. The proposed sampling
   schemes improve the reproducibility of quantitative T1 rho mapping by
   optimizing the selection of TSLs. This improvement is especially
   beneficial for longitudinal studies that track and monitor disease
   progression and treatment response.},
