@article{ WOS:000651205100011,
Author = {Tie, Wan Jun and Kent, Jacqueline Coral and Lai, Ching Tat and Rea,
   Alethea and Hepworth, Anna Rachel and Murray, Kevin and Geddes, Donna
   Tracy},
Title = {Reproducibility of the creamatocrit technique for the measurement of fat
   content in human milk},
Journal = {FOOD CHEMISTRY},
Year = {2021},
Volume = {356},
Month = {SEP 15},
Abstract = {Quantification of human milk (HM) fat is important for determining the
   energy intake of infants. The simplest and most rapid method is the
   creamatocrit method. However, the reliability of the creamatocrit has
   not been comprehensively investigated. The aims of this study were to
   test the inter- and-intra-rater reliability of: 1) HM sampling after
   hand- or-machine mixing methods and 2) HM fat measurement by the
   creamatocrit method. Interand-intra rater HM sampling after hand-
   or-machine mixing methods had high intraclass correlation coefficient (
   2\%) resulted in high variability (median coefficient of variations
   (CVs) 15\%). Intra- and inter-rater reliability of measurement of HM
   with higher fat (>3.5\%) had low variability (median CVs < 10\%). As the
   greatest variation in the creamatocrit method occurred during the
   measurement of HM samples with low fat, duplicate readings are necessary
   to reduce discrepancies in every HM fat determination.},
